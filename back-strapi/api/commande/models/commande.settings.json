{
  "kind": "collectionType",
  "collectionName": "order",
  "info": {
    "name": "Commande",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "delivery": {
      "type": "enumeration",
      "enum": [
        "home",
        "pickup",
        "referent"
      ]
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "commandes.box-sur-mesure",
        "commandes.box"
      ],
      "max": 1,
      "min": 1,
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "address_dept": {
      "type": "string"
    },
    "address_region": {
      "type": "string"
    },
    "address_zipcode": {
      "type": "string"
    },
    "poi_name": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "address_deptcode": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address_city": {
      "type": "string",
      "private": false
    },
    "address_more": {
      "type": "string"
    },
    "poi_number": {
      "type": "string"
    },
    "referent": {
      "model": "referent"
    },
    "sent": {
      "type": "boolean",
      "default": false,
      "private": false
    },
    "date_sent": {
      "type": "date"
    },
    "box_number": {
      "type": "integer"
    },
    "environnement": {
      "model": "environnement"
    },
    "received": {
      "type": "boolean",
      "default": false
    },
    "date_received": {
      "type": "datetime"
    },
    "user_data": {
      "type": "component",
      "repeatable": false,
      "component": "commandes.information-anonymes"
    },
    "additionnal_products": {
      "type": "component",
      "repeatable": true,
      "component": "commandes.box-sur-mesure-produit"
    },
    "user_notified": {
      "type": "boolean",
      "default": false
    },
    "utilisateurs_mobile": {
      "model": "utilisateurs-mobile"
    }
  }
}
